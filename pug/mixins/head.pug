mixin head(scriptPath,title)
    doctype html
    html(lang="en")
        head
            meta(charset="UTF-8")
            meta(name="viewport", content="width=device-width, initial-scale=1.0")
            script(src="./js/p5.min.js" defer)
            script(src='./js/classes.js' defer)
            script(src='./js/'+scriptPath defer)
            title title